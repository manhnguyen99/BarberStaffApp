<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.barberstaffapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BarberStaffApp">
        <activity android:name=".StaffSendOTP"></activity>
        <activity android:name=".StaffPhone" />
        <activity android:name=".StaffVerifyPhone" />
        <activity android:name=".StaffLogin" />
        <activity android:name=".StaffRegistration" />
        <activity android:name=".StaffHomeActivity" />
        <activity android:name=".SalonListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>